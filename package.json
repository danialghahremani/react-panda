    "morgan": "^1.9.1",
    "path": "^0.12.7",
    "react": "^16.9.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.1",
    "react-router": "^5.0.1",
    "react-router-config": "^5.0.1",
    "redux": "^4.0.4",
